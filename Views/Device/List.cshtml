@model IEnumerable<Device>
<h1>List of all Devices</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Vendor</th>
            <th scope="col">DateCreated</th>
            <th scope="col">Status</th>
            <th scope="col">GatewaySerialNumber</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var device in Model)
        {
            <partial name="_DeviceCard" model="device" />



        }
    </tbody>
</table>